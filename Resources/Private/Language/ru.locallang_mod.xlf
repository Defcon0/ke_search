<?xml version="1.0" encoding="UTF-8"?>
<xliff xmlns="urn:oasis:names:tc:xliff:document:1.2" version="1.2">
  <file source-language="en" datatype="plaintext" original="messages" date="2021-10-15T11:11:00Z" product-name="ke_search" target-language="ru">
    <header/>
    <body>
      <trans-unit id="mlang_tabs_tab" resname="mlang_tabs_tab">
        <source>Faceted Search</source>
        <target state="translated">Фасетный поиск</target>
      </trans-unit>
      <trans-unit id="mlang_labels_tabdescr" resname="mlang_labels_tabdescr">
        <source>Manage the search index, manually start the indexer process, see what content has been indexed, clear the the index.</source>
        <target state="translated">Управление поисковым индексом, ручной запуск процесса индексации, просмотр проиндексированного, очистка индекса.</target>
      </trans-unit>
      <trans-unit id="mlang_labels_tablabel" resname="mlang_labels_tablabel">
        <source>Faceted Search Management</source>
        <target state="translated">Управление Фасетным поиском</target>
      </trans-unit>
      <trans-unit id="title" resname="title">
        <source>Faceted Search</source>
        <target state="translated">Фасетный поиск</target>
      </trans-unit>
      <trans-unit id="function1" resname="function1">
        <source>Start indexing</source>
        <target state="translated">Запустить индексирование</target>
      </trans-unit>
      <trans-unit id="function2" resname="function2">
        <source>Indexed content</source>
        <target state="translated">Проиндексированное содержимое</target>
      </trans-unit>
      <trans-unit id="function3" resname="function3">
        <source>Index table information</source>
        <target state="translated">Информация о таблице индексов</target>
      </trans-unit>
      <trans-unit id="function4" resname="function4">
        <source>Searchword statistics</source>
        <target state="translated">Статистика поиска по словам</target>
      </trans-unit>
      <trans-unit id="function5" resname="function5">
        <source>Clear search index</source>
        <target state="translated">Очистить поисковый индекс</target>
      </trans-unit>
      <trans-unit id="function6" resname="function6">
        <source>Last indexing report</source>
        <target state="translated">Последний отчет по индексации</target>
      </trans-unit>
      <trans-unit id="select_a_page" resname="select_a_page">
        <source>Please select a page.</source>
        <target state="translated">Выберете страницу.</target>
      </trans-unit>
      <trans-unit id="not_allowed_remove_indexer_lock" resname="not_allowed_remove_indexer_lock">
        <source>You are not allowed to remove the indexer lock! This is for admins only!</source>
        <target state="translated">Вам не позволено удалить блокировку индексатора! Разрешено только администраторам!</target>
      </trans-unit>
      <trans-unit id="no_indexer_configurations" resname="no_indexer_configurations">
        <source>No indexer configurations have been found. Please create an indexer configuration record first!</source>
        <target state="translated">Конфигурации индексатора не найдены. Сначала создайте конфигурацию индексатора!</target>
      </trans-unit>
      <trans-unit id="index_contains" resname="index_contains">
        <source>The index currently contains</source>
        <target state="translated">Индекс в настоящее время содержит</target>
      </trans-unit>
      <trans-unit id="last_indexing" resname="last_indexing">
        <source>Last indexing was done on</source>
        <target state="translated">Последняя индексация была выполнена</target>
      </trans-unit>
      <trans-unit id="records" resname="records">
        <source>records</source>
        <target state="translated">записей</target>
      </trans-unit>
      <trans-unit id="page" resname="page">
        <source>page</source>
        <target state="translated">страница</target>
      </trans-unit>
      <trans-unit id="type" resname="type">
        <source>type</source>
        <target state="translated">тип</target>
      </trans-unit>
      <trans-unit id="configurations_found" resname="configurations_found">
        <source>Indexer configurations have been found:</source>
        <target state="translated">Найдены конфигурации индексатора:</target>
      </trans-unit>
      <trans-unit id="tag_too_short" resname="tag_too_short">
        <source>Tag too short</source>
        <target state="translated">Метка слишком короткая</target>
      </trans-unit>
      <trans-unit id="tag_too_short_message" resname="tag_too_short_message">
        <source>The given tag &quot;%s&quot; is too short. A minimum length of %d is required. See extension setting &quot;searchWordLength&quot; for further info.</source>
        <target state="translated">Тег "%s" слишком короткий. Минимальная длина %d. За информацией обратитесь к настройке расширения "searchWordLength".</target>
      </trans-unit>
    </body>
  </file>
